    "sw-precache-webpack-plugin": "^0.11.3",
    "webpack": "^2.2.1"
  },
  "keywords": [],
  "scripts": {
    "build": "webpack -p",
    "watch": "webpack -w -p",
    "start": "concurrent 'npm run watch' 'npm run jekyll'",
    "new": "gem install bundler ; bundle install ; npm prune ; npm install --no-optional",
    "deploy": "jekyll build ; npm run build ; npm run now",
    "jekyll:serve": "jekyll serve --watch",
    "jekyll:watch": "jekyll build --watch",
    "prenow": "npm run build",
    "now": "now _site -n zander.wtf --alias zander.wtf"
  },
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "modules": false
        }
      ]
    ]
  },
  "now": {
    "alias": "zander.wtf"
  }
}
