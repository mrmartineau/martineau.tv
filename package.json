{
  "name": "zander.wtf",
  "title": "zander.wtf",
  "description": "Zander's personal website",
  "version": "1.0.0",
  "homepage": "http://zander.wtf",
  "author": {
    "name": "Zander Martineau",
    "url": "http://zander.wtf"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "dependencies": {
    "normalize.css": "^5.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.6",
    "css-loader": "^0.26.2",
    "cssnano": "^3.6.2",
    "extract-text-webpack-plugin": "^2.0.0",
    "lost": "^8.0.0",
    "postcss-advanced-variables": "^1.2.2",
    "postcss-compact-mq": "^0.2.1",
    "postcss-custom-properties": "^5.0.2",
    "postcss-easings": "^0.3.0",
    "postcss-easy-import": "^2.0.0",
    "postcss-loader": "^1.3.3",
    "postcss-mixins": "^5.4.1",
    "postcss-modular-scale": "^2.5.2",
    "postcss-nested": "^1.0.0",
    "postcss-nested-ancestors": "^1.0.0",
    "postcss-reporter": "^3.0.0",
    "resolve-url-loader": "^2.0.1",
    "style-loader": "^0.13.2",
    "webpack": "^2.2.1"
  },
  "keywords": [],
  "scripts": {
    "build": "webpack -p",
    "watch": "webpack -w",
    "start": "concurrent 'npm run watch' 'npm run jekyll'",
    "new": "gem install bundler ; bundle install ; npm prune ; npm install --no-optional",
    "testdeploy": "jekyll build ; grunt compile --release ; now _site -n zander.wtf",
    "deploy": "jekyll build ; npm run build ; npm run now",
    "jekyll": "jekyll serve --watch",
    "prenow": "npm run build",
    "now": "now _site -n zander.wtf --alias zander.wtf"
  }
}
